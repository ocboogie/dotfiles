  "Function Definition": {
    "prefix": "fund",
    "body": "$1:$2\\rightarrow $0"
  },
  "Cyclic": {
    "prefix": "cyc",
    "body": "\\langle $1\\rangle $0"
  },
  "Align": {
    "prefix": "al",
    "body": [
