  "Math remove": {
    "prefix": "mr",
    "body": "\\mathrm{${1:$TM_SELECTED_TEXT}}$0"
  },
  "Text": {
    "prefix": "mt",
    "body": "\\text{${1:$TM_SELECTED_TEXT}}$0"
