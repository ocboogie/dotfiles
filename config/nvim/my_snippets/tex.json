      "  \\textbf{Proposition ($1).} $2",
      "\\end{tcolorbox}$0"
    ]
  },
  "Definition": {
    "prefix": "def",
    "body": [
      "",
      "\\begin{tcolorbox}",
      "  \\textbf{Definition ($1).} $2",
      "\\end{tcolorbox}$0"
    ]
  },
  "Intuition": {
    "prefix": "intu",
    "body": [
      "",
      "\\begin{tcolorbox}",
      "  \\textbf{Intuition} $1",
      "\\end{tcolorbox}$0"
    ]
  },
  "Theorem": {
    "prefix": "thm",
    "body": [
      "",
      "\\begin{tcolorbox}",
      "  \\textbf{Theorem ($1).} $2",
      "\\end{tcolorbox}$0"
    ]
  },
  "Example": {
    "prefix": "ex",
    "body": [
      "",
      "\\begin{tcolorbox}",
      "  \\textbf{Example.} $2",
      "\\end{tcolorbox}$0"
    ]
  },
  "Corollary": {
    "prefix": "cor",
    "body": [
      "",
      "\\begin{tcolorbox}",
      "  \\textbf{Corollary ($1).} $2",
      "\\end{tcolorbox}$0"
    ]
  },
  "Remark": {
    "prefix": "rem",
    "body": [
      "",
      "\\begin{tcolorbox}",
      "  \\textbf{Remark.} $1",
      "\\end{tcolorbox}$0"
    ]
  },
  "Inline math": {
    "prefix": "ie",
    "body": [
      "\\$$1\\$$0"
    ]
  },
  "Display math": {
    "prefix": "dm",
    "body": [
      "\\[",
      "  $1",
      "\\]$0"
    ]
  },
  "Boldface text": {
    "prefix": "bf",
    "body": "\\textbf{${1:$TM_SELECTED_TEXT}}$0"
  },
  "Blackboard-bold text": {
    "prefix": "bb",
    "body": "\\mathbb{${1:$TM_SELECTED_TEXT}}$0"
