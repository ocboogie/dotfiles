{
  "Boldface math": {
    "prefix": "ts",
    "body": "\\mathbf{${1:$TM_SELECTED_TEXT}}$0"
  },
  "Fraction": {
    "prefix": "/",
    "body": "\\frac{${1:$TM_SELECTED_TEXT}}{$2}$0"
  },
  "Italics": {
    "prefix": "it",
    "body": "\\textit{$1}$0"
  },
  "Part": {
    "prefix": "part",
    "body": [
      "\\noindent",
      "\\textbf{Part $1.} $0"
    ]
  },
  "Solution": {
    "prefix": "sol",
    "body": "\\textbf{Solution.} $0"
  },
  "Begin": {
    "prefix": "beg",
    "body": [
      "\\begin{$1}",
      "  $0",
      "\\end{$1}"
    ]
  },
  "3x3 Matrix": {
    "prefix": "m3",
    "body": [
      "\\begin{${1:pmatrix}}",
      "$2 & $3 & $4 \\\\",
      "$5 & $6 & $7 \\\\",
      "$8 & $9 & $10",
      "\\end{${1:pmatrix}}$0"
    ]
  },
  "Box": {
    "prefix": "box",
    "body": [
      "",
      "\\begin{tcolorbox}",
      "  \\textbf{$1.} $2",
      "\\end{tcolorbox}$0"
    ]
  },
  "Summary": {
    "prefix": "sum",
    "body": [
      "",
      "\\begin{tcolorbox}",
      "  \\textbf{Summary.} $1",
      "\\end{tcolorbox}$0"
    ]
  },
  "Lemma": {
    "prefix": "lem",
    "body": [
      "",
      "\\begin{tcolorbox}",
      "  \\textbf{Lemma ($1).} $2",
      "\\end{tcolorbox}$0"
    ]
  },
  "Proposition": {
    "prefix": "prop",
    "body": [
      "",
